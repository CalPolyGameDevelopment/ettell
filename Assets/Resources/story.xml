<?xml version="1.0" encoding="utf-8"?>
<story>
	<node id="firstPuzzle">
		<minigame data="dialog">
			<prompt data="Current year: \year" />
			<prompt data="Isaac@AI:~/# ./Attell.py 'Who are you?'" />
			<response data="Syntax error: 'Who' on line 1" edge="firstPuzzleRecur" />
			<response data="Filename: Attell.py" edge="firstPuzzleRecur" />
			<response data="Kernel ID: 2.6.27-7-generic" edge="firstPuzzleRecur" />
			<response data="OS: Ubuntu 8.10" edge="firstPuzzleRecur" />
			<response data="I don't know, who am I?" edge="winGame" />
		</minigame>
	</node>
	<node id="youWin">
		<minigame data="dialog">
			<prompt data="You win!" />
		</minigame>
	</node>
	<edge id="firstPuzzleRecur" data="firstPuzzle">
		<consequence>
			<change data="year">
				<add data="2" />
			</change>
			<change data="cpuPower">
				<multiply data="2" />
			</change>
		</consequence>
	</edge>
	<edge id="winGame" data="youWin" />
</story>	